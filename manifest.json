{
  "manifest_version": 2,
  "name": "Github Approve Shortcut",
  "version": "1.0",
  "description": "Hate your mouse? Me too!",
  "permissions": ["https://github.com/", "activeTab"],
  "content_scripts": [
    {
      "matches": ["https://github.com/*/*/pull/*"],
      "js": ["content.js"]
    }
  ]
}

